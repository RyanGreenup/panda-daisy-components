{
  "name": "@ryangreenup/panda-daisy-components",
  "version": "0.0.0",
  "description": "SolidJS UI components with PandaCSS styling",
  "type": "module",
  "keywords": [
    "solid",
    "solidjs",
    "ui",
    "components",
    "pandacss"
  ],
  "main": "src/index.ts",
  "workspaces": [
    "packages/*"
  ],
  "module": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    "./dist": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "solid": "./dist/*index.jsx"
    },
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./presets/*": {
      "import": "./src/presets/*.ts",
      "types": "./src/presets/*.ts"
    }
  },
  "files": [
    "src"
  ],
  "scripts": {
    "storybook": "storybook dev -p 6006",
    "build:ship": "panda ship --outfile dist/panda.buildinfo.json",
    "build:cssgen": "panda cssgen --outfile dist/styles.css",
    "build:tsdown": "tsdown",
    "build:storybook": "storybook build",
    "build": "npm run build:ship && npm run build:cssgen && npm run build:tsdown && npm run build:storybook",
    "build-storybook": "storybook build",
    "prepare": "panda codegen",
    "prepare-clean": "panda codegen --clean"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@pandacss/dev": "^1.2.0",
    "@storybook/addon-a11y": "^9.1.5",
    "@storybook/addon-docs": "^9.1.5",
    "@storybook/addon-links": "^9.1.5",
    "@storybook/addon-onboarding": "^9.1.5",
    "@storybook/addon-vitest": "^9.1.5",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "playwright": "^1.55.0",
    "storybook": "^9.1.5",
    "storybook-solidjs-vite": "^9.0.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@kobalte/core": "^0.13.11",
    "@tanstack/solid-table": "^8.21.3",
    "@tanstack/solid-virtual": "^3.13.12",
    "lucide-solid": "^0.500.0",
    "rolldown-plugin-solid": "^0.2.1",
    "solid-js": "^1.9.9",
    "solid-transition-group": "^0.3.0",
    "tsdown": "^0.14.2"
  },
  "peerDependencies": {
    "@kobalte/core": "^0.13.11",
    "solid-js": "^1.9.0",
    "@tanstack/solid-virtual": "^3.0.0",
    "lucide-solid": "^0.500.0",
    "solid-transition-group": "^0.3.0"
  }
}
